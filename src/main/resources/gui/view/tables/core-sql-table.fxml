<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="900" prefWidth="1300" spacing="8" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.controllers.tables.CoreTableSQLController">

    <HBox alignment="CENTER" prefHeight="51.0" prefWidth="1237.0" spacing="70.0">
        <children>
         <Label fx:id="tableTitle" prefHeight="40.0" prefWidth="433.0" text="Table Title">
            <font>
               <Font size="28.0" />
            </font>
            <HBox.margin>
               <Insets right="100.0" />
            </HBox.margin>
         </Label>
            <Button fx:id="addButton" onAction="#addRowPopup" prefHeight="51.0" prefWidth="198.0" text="Add Item">
                <font>
                    <Font name="System Bold" size="24.0" />
                </font>
            <HBox.margin>
               <Insets left="60.0" right="30.0" />
            </HBox.margin>
            </Button>
         <Button fx:id="returnButton1" layoutX="710.0" layoutY="10.0" onAction="#returnToMainMenu" prefHeight="51.0" prefWidth="198.0" text="Return">
            <font>
               <Font name="System Bold" size="24.0" />
            </font>
         </Button>
        </children>
        <VBox.margin>
            <Insets bottom="10.0" left="55.0" top="20.0" />
        </VBox.margin>
    </HBox>

    <TableView fx:id="table" editable="true" prefHeight="811.0" prefWidth="1300.0" stylesheets="@table-style.css" />
</VBox>
